{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ccxt/ccxt": "^1.83",
        "coingate/coingate-php": "^3.0",
        "cviebrock/eloquent-sluggable": "^8.0",
        "ezyang/htmlpurifier": "^4.13",
        "fideloper/proxy": "^4.4",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": "1.8.3",
        "intervention/image": "^2.5",
        "jamesmills/laravel-timezone": "^1.9",
        "kornrunner/keccak": "^1.1",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.4",
        "laravel/sanctum": "^2.12",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.8",
        "mobiledetect/mobiledetectlib": "^2.8",
        "mollie/laravel-mollie": "^2.17",
        "paypal/paypal-checkout-sdk": "^1.0",
        "predis/predis": "^1.1",
        "razorpay/razorpay": "^2.5",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "stripe/stripe-php": "^7.72",
        "tatumio/tatum-php": "^1.0",
        "webdevetc/blogetc": "^8.4",
        "yajra/laravel-datatables-oracle": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "pixinvent/jetstrap-vuexy": "dev-main",
        "sven/artisan-view": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/Helpers/helpers.php"
        ]
    },
    "repositories": {
        "tatumio/tatum-php": {
            "type": "path",
            "url": "app/Http/Controllers/Admin/Eco/tatum"
        },
        "bitwasp/bitcoin": {
            "type": "path",
            "url": "vendor/bitwasp"
        },
        "btccom/bitwasp-bitcoin-bch-addon": {
            "type": "path",
            "url": "vendor/btccom/bitwasp-bitcoin-bch-addon-master"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
